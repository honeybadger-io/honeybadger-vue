{
  "name": "@honeybadger-io/vue",
  "version": "0.0.9",
  "license": "MIT",
  "description": "Honeybadger Vue.js client",
  "author": "Jason Truesdell <jason@yuzuten.com> (https://github.com/JasonTrue)",
  "bugs": {
    "url": "https://github.com/honeybadger-io/honeybadger-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/honeybadger-io/honeybadger-vue.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "honeybadger"
  ],
  "main": "dist/honeybadger-vue.umd.js",
  "module": "dist/honeybadger-vue.umd.js",
  "unpkg": "dist/honeybadger-vue.min.js",
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/honeybadger-io/honeybadger-vue#readme",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit",
    "test:all": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/honeybadger-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/honeybadger-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/honeybadger-vue.js",
    "build:unpkg-minify": "rollup MINIFY=true --config build/rollup.config.js --format iife --file dist/honeybadger-vue.min.js",
    "preversion": "npm test",
    "version": "scripts/update-versions.sh",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "honeybadger-js": "^2.0.0"
  },
  "devDependencies": {
    "ajv": "^6.5.4",
    "autoprefixer": "^9.7.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^3.0.0",
    "chromedriver": "^79.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^7.0.1",
    "css-loader": "^0.28.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "interactive": "^0.1.9",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "nightwatch": "^1.3.2",
    "nightwatch-xhr": "^0.4.6",
    "node-notifier": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.3",
    "portfinder": "^1.0.13",
    "postcss": "^7.0.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.13",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-conditional": "^3.1.2",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.4",
    "selenium-server": "^3.0.1",
    "semver": "^7.1.1",
    "shelljs": "^0.8.3",
    "sinon": "^8.0.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0",
    "winston": "^3.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "directories": {
    "test": "test"
  },
  "resolutions": {
    "debug": "^2.2.1",
    "growl": "^1.10.5",
    "har-validator": "^5.1.3",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "socks-proxy-agent": "^4.0.1"
  },
  "types": "./index.d.ts"
}
